{
  "name": "scanner-api",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "npm run test:lint && npm run test:coverage ",
    "build": "npm run barrels && tsc && babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "npm run barrels && babel-watch --extensions .ts src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "npm run test:unit ",
    "prisma:migrate:dev": "npx prisma migrate dev --name init",
    "prisma:migrate": "npx prisma migrate deploy",
    "prisma:generate": "npx prisma generate",
    "prisma:seed": "ts-node --transpile-only prisma/seed.ts",
    "lint": "eslint 'src/**/*.{ts,tsx}' --quiet --fix"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  },
